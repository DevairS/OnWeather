{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/api": [
        "src/api"
      ],
      "~/components": [
        "src/components"
      ],
      "~/enum": [
        "src/enum"
      ],
      "~/hooks": [
        "src/hooks"
      ],
      "~/models": [
        "src/models"
      ],
      "~/screens": [
        "src/screens"
      ],
      "~/routes": [
        "src/routes"
      ],
      "~/stores": [
        "src/stores"
      ],
      "~/theme": [
        "src/theme"
      ],
      "~/types/*": [
        "src/types/*"
      ],
      "~/utils": [
        "src/utils"
      ]
    }
  }
}