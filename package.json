{
  "name": "on-weather",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios14": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "tsc && eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix --max-warnings 100",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "react-native setup-ios-permissions && pod-install",
    "extract-compile": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' [ --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file src/lang/pt_br.json && rm temp.json"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "1.2.2",
    "mobx": "^6.9.0",
    "mobx-react": "^8.0.0",
    "mobx-react-lite": "^4.0.2",
    "react": "18.2.0",
    "react-intl": "^6.3.2",
    "react-native": "0.71.4",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-permissions": "^3.8.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@formatjs/cli": "^6.0.4",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "5.1.6",
    "@types/styled-components-react-native": "5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "pod-install": "^0.1.38",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.8",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "reactNativePermissionsIOS": [
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse"
  ]
}